{
  "name": "mikepsinn/php-highcharts-exporter",
  "license": "MIT",
  "description": "Export highcharts from PHP without running the NodeJS export server",
  "type": "library",
  "keywords": [
    "highcharts-export-server",
    "highcharts"
  ],
  "require": {
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "MikeSinn\\HighchartsExporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MikeSinn\\HighchartsExporter\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests",
    "post-install-cmd": [
      "set -x && sudo chmod +x vendor/mikepsinn/php-highcharts-exporter/phantomjs"
    ],
    "post-update-cmd": [
      "set -x && sudo chmod +x vendor/mikepsinn/php-highcharts-exporter/phantomjs"
    ]
  },
  "authors": [
    {
      "name": "Mike Sinn",
      "email": "m@quantimodo.com"
    }
  ]
}
